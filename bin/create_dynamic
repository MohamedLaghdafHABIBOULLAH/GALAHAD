mkdir -p dynamic

gcc -O -shared -o dynamic/libgalahad_py.so -Wl,--whole-archive libgalahad_py.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_c.so -Wl,--whole-archive libgalahad_c.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_hsl_c.so -Wl,--whole-archive libgalahad_hsl_c.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad.so -Wl,--whole-archive libgalahad.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_hsl.so -Wl,--whole-archive libgalahad_hsl.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_spral.so -Wl,--whole-archive libgalahad_spral.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_mkl_pardiso.so -Wl,--whole-archive libgalahad_mkl_pardiso.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_pardiso.so -Wl,--whole-archive libgalahad_pardiso.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_wsmp.so -Wl,--whole-archive libgalahad_wsmp.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_metis.so -Wl,--whole-archive libgalahad_metis.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_lapack.so -Wl,--whole-archive libgalahad_lapack.a -Wl,--no-whole-archive
gfortran -O -shared -o dynamic/libgalahad_blas.so -Wl,--whole-archive libgalahad_blas.a -Wl,--no-whole-archive
