	$(CP) $(HSL)/mi26/mi26d.f $(OBJ)/mi26d.f90
	cd $(OBJ); $(FORTRAN) -o mi26d.o $(FFLAGS77) mi26d.f90
	cd $(OBJ); $(HARR) mi26d.o ; $(RM) mi26d.f90 mi26d.o
	@printf '[ OK ]\n'

