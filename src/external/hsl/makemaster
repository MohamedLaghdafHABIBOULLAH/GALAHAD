             $(HLGS)(mc59s.o) $(HLGS)(la15s.o) $(HLGS)(la04s.o)
la04_double: $(HLGD)(fa14d.o) $(HLGD)(fd15d.o) $(HLGD)(mc29d.o) \
             $(HLGD)(mc59d.o) $(HLGD)(la15d.o) $(HLGD)(la04d.o)
la04_single_64: $(HLGS64)(fa14s.o) $(HLGS64)(fd15s.o) $(HLGS64)(mc29s.o) \
                $(HLGS64)(mc59s.o) $(HLGS64)(la15s.o) $(HLGS64)(la04s.o)
la04_double_64: $(HLGD64)(fa14d.o) $(HLGD64)(fd15d.o) $(HLGD64)(mc29d.o) \
                $(HLGD64)(mc59d.o) $(HLGD64)(la15d.o) $(HLGD64)(la04d.o)

