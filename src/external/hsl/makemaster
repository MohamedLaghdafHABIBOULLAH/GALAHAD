ad02_double: $(HLGD)(hsl_ad02d.o)
ad02_single_64: $(HLGS64)(hsl_ad02s.o)
ad02_double_64: $(HLGD64)(hsl_ad02d.o)

#  la04

la04: la04_$(PRECIS)
	@printf ' %-21s\n' "HSL LA04 routines ($(PRECIS) $(SUCC)"
