#  other packages

blas:
	( cd $(GALAHAD)/src/general ; \
             $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
             blas_silent PRECIS=$(PRECIS) PWD=$(GALAHAD)/src/general )
lapack:
	( cd $(GALAHAD)/src/general ; \
             $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
             lapack_silent PRECIS=$(PRECIS) PWD=$(GALAHAD)/src/general )
metis:
	( cd $(GALAHAD)/src/general ; \
             $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
            metis_silent PRECIS=$(PRECIS) PWD=$(GALAHAD)/src/general )
kinds:
	( cd $(GALAHAD)/src/kinds ; \
             $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
             kinds_silent PRECIS=$(PRECIS) PWD=$(GALAHAD)/src/kinds )
zd11:
	( cd $(GALAHAD)/src/zd11 ; \
             $(MAKE) -f $(GALAHAD)/makefiles/$(VERSION) \
             zd11_silent PRECIS=$(PRECIS) PWD=$(GALAHAD)/src/zd11 )

#  =======================
#  individual compilations
#  =======================

#  hsl_ad02

hsl_ad02d.o: $(HLG)(hsl_ad02d.o)

$(HLG)(hsl_ad02d.o): $(HSL_ARCHIVE)/hsl_ad02/hsl_ad02d.f90
	@printf ' %-9s %-15s\t\t' "Compiling" "hsl_ad02d"
	$(CP) $(HSL_ARCHIVE)/hsl_ad02/hsl_ad02d.f90 \
           $(OBJ)/hsl_ad02d.$(F95SUFFIX)
	cd $(OBJ); $(FORTRAN) $(FFLAGS95) hsl_ad02d.$(F95SUFFIX) \
                || ( printf ' %-26s' "=> Disabling optimization " ; \
                   $(FORTRAN) $(FFLAGS95N) hsl_ad02d.$(F95SUFFIX) )
	cd $(OBJ); $(HARR) hsl_ad02d.o; $(RM) hsl_ad02d.$(F95SUFFIX) hsl_ad02d.o
	$(RMARFILE) uselanb.o
	$(MVMODS)
	$(RANLIB) $(HLG)
	@printf '[ OK ]\n'

hsl_ad02s.o: $(HLG)(hsl_ad02s.o)

$(HLG)(hsl_ad02s.o): $(HSL_ARCHIVE)/hsl_ad02/hsl_ad02s.f90
	@printf ' %-9s %-15s\t\t' "Compiling" "hsl_ad02s"
	$(CP) $(HSL_ARCHIVE)/hsl_ad02/hsl_ad02s.f90 \
           $(OBJ)/hsl_ad02s.$(F95SUFFIX)
	cd $(OBJ); $(FORTRAN) $(FFLAGS95) hsl_ad02s.$(F95SUFFIX) \
                || ( printf ' %-26s' "=> Disabling optimization " ; \
                   $(FORTRAN) $(FFLAGS95N) hsl_ad02s.$(F95SUFFIX) )
	cd $(OBJ); $(HARR) hsl_ad02s.o; $(RM) hsl_ad02s.$(F95SUFFIX) hsl_ad02s.o
	$(RMARFILE) uselanb.o
	$(MVMODS)
	$(RANLIB) $(HLG)
	@printf '[ OK ]\n'

