                      $(HCLGS64)(hsl_mi28s_ciface.o)
hsl_ciface_double_64: $(HCLGD64)(hsl_ma48d_ciface.o) \
                      $(HCLGD64)(hsl_ma57d_ciface.o) \
                      $(HCLGD64)(hsl_ma77d_ciface.o) \
                      $(HCLGD64)(hsl_ma86d_ciface.o) \
                      $(HCLGD64)(hsl_ma87d_ciface.o) \
                      $(HCLGD64)(hsl_ma97d_ciface.o) \
                      $(HCLGD64)(hsl_mi20d_ciface.o) \
