             $(HLGD)(hsl_mc68i.o) $(HLGD)(hsl_mc69d.o) $(HLGD)(mc64d.o) \
             $(HLGD)(hsl_mc64d.o) $(HLGD)(mc30d.o) $(HLGD)(mc77d.o)     \
             $(HLGD)(hsl_mc78i.o) $(HLGD)(hsl_mc80d.o) $(HLGD)(hsl_ma97d.o)
ma97_single_64: blas lapack $(HLGS64)(hsl_mc34s.o) $(HLGS64)(hsl_zb01i.o) \
                $(HLGS64)(hsl_mc68i.o) $(HLGS64)(hsl_mc69s.o) \
                $(HLGS64)(mc64s.o) $(HLGS64)(hsl_mc64s.o) $(HLGS64)(mc30s.o) \
                $(HLGS64)(mc77s.o) $(HLGS64)(hsl_mc78i.o) \
                $(HLGS64)(hsl_mc80s.o) $(HLGS64)(hsl_ma97s.o)
ma97_double_64: blas lapack $(HLGD64)(hsl_mc34d.o) $(HLGD64)(hsl_zb01i.o) \
                $(HLGD64)(hsl_mc68i.o) $(HLGD64)(hsl_mc69d.o) \
                $(HLGD64)(mc64d.o) $(HLGD64)(hsl_mc64d.o) $(HLGD64)(mc30d.o) \
                $(HLGD64)(mc77d.o) $(HLGD64)(hsl_mc78i.o) \
                $(HLGD64)(hsl_mc80d.o) $(HLGD64)(hsl_ma97d.o)

ma97_ciface: ma97_ciface_$(PRECIS)
	@printf ' %-21s\n' "HSL MA48 C routines ($(PRECIS) $(SUCC)"
ma97_ciface_silent: ma97_ciface_$(PRECIS)
ma97_ciface_single: ma97_single $(HCLGS)(hsl_ma97s_ciface.o)
ma97_ciface_double: ma97_double $(HCLGD)(hsl_ma97d_ciface.o)
ma97_ciface_single_64: ma97_single_64 $(HCLGS64)(hsl_ma97s_ciface.o)
ma97_ciface_double_64: ma97_double_64 $(HCLGD64)(hsl_ma97d_ciface.o)

