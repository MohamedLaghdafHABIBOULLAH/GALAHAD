#  Main body of the installation makefile for GALAHAD's HSL packages

#  N. Gould
#  This version: 2024-02-12

#  include standard GALAHAD HSL makefile definitions

include $(GALAHAD)/src/makedefs/hsl_definitions

#  ===================
#  compilation agendas
#  ===================

#  all packages

hsl: hsl_$(PRECIS)
	@printf ' %-21s\n' "HSL routines ($(PRECIS) $(SUCC)"
hsl_silent: hsl_$(PRECIS)
hsl_single: blas lapack metis kinds zd11 \
            $(HLGS)(fa14s.o) $(HLGS)(fd15s.o) $(HLGS)(kb07i.o) \
            $(HLGS)(mc13s.o) $(HLGS)(mc20s.o) $(HLGS)(mc21s.o) \
            $(HLGS)(mc29s.o) $(HLGS)(mc30s.o) $(HLGS)(mc34s.o) \
            $(HLGS)(mc47s.o) $(HLGS)(mc59s.o) $(HLGS)(mc61s.o) \
            $(HLGS)(mc64s.o) $(HLGS)(mc71s.o) $(HLGS)(mc77s.o) \
            $(HLGS)(la15s.o) $(HLGS)(la04s.o) $(HLGS)(ma27s.o) \
            $(HLGS)(ma33s.o) $(HLGS)(ma57s.o) $(HLGS)(ma61s.o) \
            $(HLGS)(mi21s.o) $(HLGS)(mi24s.o) $(HLGS)(mi26s.o) \
            $(HLGS)(hsl_zb01s.o) $(HLGS)(hsl_zb01i.o) \
            $(HLGS)(hsl_kb22l.o) $(HLGS)(hsl_mc34s.o) \
            $(HLGS)(hsl_of01i.o) $(HLGS)(hsl_of01s.o) \
            $(HLGS)(hsl_mc65s.o) $(HLGS)(hsl_mc68i.o) \
            $(HLGS)(hsl_mc69s.o) $(HLGS)(hsl_mc64s.o) \
            $(HLGS)(hsl_mc78i.o) $(HLGS)(hsl_mc80s.o) \
            $(HLGS)(hsl_ma48s.o) $(HLGS)(hsl_ma57s.o) \
            $(HLGS)(hsl_ma54s.o) $(HLGS)(hsl_ma64s.o) \
            $(HLGS)(hsl_ma77s.o) $(HLGS)(hsl_ad02s.o) \
            $(HLGS)(hsl_ma86s.o) $(HLGS)(hsl_ma87s.o) \
            $(HLGS)(hsl_ma97s.o) $(HLGS)(hsl_mi32s.o) \
            $(HLGS)(hsl_mi20s.o) $(HLGS)(hsl_mi28s.o) \
            $(HLGS)(hsl_mi35s.o)
hsl_double: blas lapack metis kinds zd11 \
            $(HLGD)(fa14d.o) $(HLGD)(fd15d.o) $(HLGD)(kb07i.o) \
            $(HLGD)(mc13d.o) $(HLGD)(mc20d.o) $(HLGD)(mc21d.o) \
            $(HLGD)(mc29d.o) $(HLGD)(mc30d.o) $(HLGD)(mc34d.o) \
            $(HLGD)(mc47d.o) $(HLGD)(mc59d.o) $(HLGD)(mc61d.o) \
            $(HLGD)(mc64d.o) $(HLGD)(mc71d.o) $(HLGD)(mc77d.o) \
            $(HLGD)(la15d.o) $(HLGD)(la04d.o) $(HLGD)(ma27d.o) \
            $(HLGD)(ma33d.o) $(HLGD)(ma57d.o) $(HLGD)(ma61d.o) \
            $(HLGD)(mi21d.o) $(HLGD)(mi24d.o) $(HLGD)(mi26d.o) \
            $(HLGD)(hsl_zb01d.o) $(HLGD)(hsl_zb01i.o) \
            $(HLGD)(hsl_kb22l.o) $(HLGD)(hsl_mc34d.o) \
            $(HLGD)(hsl_of01i.o) $(HLGD)(hsl_of01d.o) \
            $(HLGD)(hsl_mc65d.o) $(HLGD)(hsl_mc68i.o) \
            $(HLGD)(hsl_mc69d.o) $(HLGD)(hsl_mc64d.o) \
            $(HLGD)(hsl_mc78i.o) $(HLGD)(hsl_mc80d.o) \
            $(HLGD)(hsl_ma48d.o) $(HLGD)(hsl_ma57d.o) \
            $(HLGD)(hsl_ma54d.o) $(HLGD)(hsl_ma64d.o) \
            $(HLGD)(hsl_ma77d.o) $(HLGD)(hsl_ad02d.o) \
            $(HLGD)(hsl_ma86d.o) $(HLGD)(hsl_ma87d.o) \
            $(HLGD)(hsl_ma97d.o) $(HLGD)(hsl_mi32d.o) \
            $(HLGD)(hsl_mi20d.o) $(HLGD)(hsl_mi28d.o) \
            $(HLGD)(hsl_mi35d.o)

hsl_single_64: blas lapack metis kinds zd11 \
               $(HLGS64)(fa14s.o) $(HLGS64)(fd15s.o) $(HLGS64)(kb07i.o) \
               $(HLGS64)(mc13s.o) $(HLGS64)(mc20s.o) $(HLGS64)(mc21s.o) \
               $(HLGS64)(mc29s.o) $(HLGS64)(mc30s.o) $(HLGS64)(mc34s.o) \
               $(HLGS64)(mc47s.o) $(HLGS64)(mc59s.o) $(HLGS64)(mc61s.o) \
               $(HLGS64)(mc64s.o) $(HLGS64)(mc71s.o) $(HLGS64)(mc77s.o) \
               $(HLGS64)(la15s.o) $(HLGS64)(la04s.o) $(HLGS64)(ma27s.o) \
               $(HLGS64)(ma33s.o) $(HLGS64)(ma57s.o) $(HLGS64)(ma61s.o) \
               $(HLGS64)(mi21s.o) $(HLGS64)(mi24s.o) $(HLGS64)(mi26s.o) \
               $(HLGS64)(hsl_zb01s.o) $(HLGS64)(hsl_zb01i.o) \
               $(HLGS64)(hsl_kb22l.o) $(HLGS64)(hsl_mc34s.o) \
               $(HLGS64)(hsl_of01i.o) $(HLGS64)(hsl_of01s.o) \
               $(HLGS64)(hsl_mc65s.o) $(HLGS64)(hsl_mc68i.o) \
               $(HLGS64)(hsl_mc69s.o) $(HLGS64)(hsl_mc64s.o) \
               $(HLGS64)(hsl_mc78i.o) $(HLGS64)(hsl_mc80s.o) \
               $(HLGS64)(hsl_ma48s.o) $(HLGS64)(hsl_ma57s.o) \
               $(HLGS64)(hsl_ma54s.o) $(HLGS64)(hsl_ma64s.o) \
               $(HLGS64)(hsl_ma77s.o) $(HLGS64)(hsl_ad02s.o) \
               $(HLGS64)(hsl_ma86s.o) $(HLGS64)(hsl_ma87s.o) \
               $(HLGS64)(hsl_ma97s.o) $(HLGS64)(hsl_mi32s.o) \
               $(HLGS64)(hsl_mi20s.o) $(HLGS64)(hsl_mi28s.o) \
               $(HLGS64)(hsl_mi35s.o)
hsl_double_64: blas lapack metis kinds zd11 \
               $(HLGD64)(fa14d.o) $(HLGD64)(fd15d.o) $(HLGD64)(kb07i.o) \
               $(HLGD64)(mc13d.o) $(HLGD64)(mc20d.o) $(HLGD64)(mc21d.o) \
               $(HLGD64)(mc29d.o) $(HLGD64)(mc30d.o) $(HLGD64)(mc34d.o) \
               $(HLGD64)(mc47d.o) $(HLGD64)(mc59d.o) $(HLGD64)(mc61d.o) \
               $(HLGD64)(mc64d.o) $(HLGD64)(mc71d.o) $(HLGD64)(mc77d.o) \
               $(HLGD64)(la15d.o) $(HLGD64)(la04d.o) $(HLGD64)(ma27d.o) \
               $(HLGD64)(ma33d.o) $(HLGD64)(ma57d.o) $(HLGD64)(ma61d.o) \
               $(HLGD64)(mi21d.o) $(HLGD64)(mi24d.o) $(HLGD64)(mi26d.o) \
               $(HLGD64)(hsl_zb01d.o) $(HLGD64)(hsl_zb01i.o) \
               $(HLGD64)(hsl_kb22l.o) $(HLGD64)(hsl_mc34d.o) \
               $(HLGD64)(hsl_of01i.o) $(HLGD64)(hsl_of01d.o) \
               $(HLGD64)(hsl_mc65d.o) $(HLGD64)(hsl_mc68i.o) \
               $(HLGD64)(hsl_mc69d.o) $(HLGD64)(hsl_mc64d.o) \
               $(HLGD64)(hsl_mc78i.o) $(HLGD64)(hsl_mc80d.o) \
               $(HLGD64)(hsl_ma48d.o) $(HLGD64)(hsl_ma57d.o) \
               $(HLGD64)(hsl_ma54d.o) $(HLGD64)(hsl_ma64d.o) \
               $(HLGD64)(hsl_ma77d.o) $(HLGD64)(hsl_ad02d.o) \
               $(HLGD64)(hsl_ma86d.o) $(HLGD64)(hsl_ma87d.o) \
               $(HLGD64)(hsl_ma97d.o) $(HLGD64)(hsl_mi32d.o) \
               $(HLGD64)(hsl_mi20d.o) $(HLGD64)(hsl_mi28d.o) \
               $(HLGD64)(hsl_mi35d.o)
             
hsl_ciface: hsl_$(PRECIS) hsl_ciface_$(PRECIS)
	@printf ' %-21s\n' "HSL C routines ($(PRECIS) $(SUCC)"
hsl_ciface_silent: hsl_$(PRECIS) hsl_ciface_$(PRECIS)
hsl_ciface_single: $(HCLGS)(hsl_ma48s_ciface.o) \
                   $(HCLGS)(hsl_ma57s_ciface.o) \
                   $(HCLGS)(hsl_ma77s_ciface.o) \
                   $(HCLGS)(hsl_ma86s_ciface.o) \
                   $(HCLGS)(hsl_ma87s_ciface.o) \
                   $(HCLGS)(hsl_ma97s_ciface.o) \
                   $(HCLGS)(hsl_mi20s_ciface.o) \
                   $(HCLGS)(hsl_mi28s_ciface.o)
hsl_ciface_double: $(HCLGD)(hsl_ma48d_ciface.o) \
                   $(HCLGD)(hsl_ma57d_ciface.o) \
                   $(HCLGD)(hsl_ma77d_ciface.o) \
                   $(HCLGD)(hsl_ma86d_ciface.o) \
                   $(HCLGD)(hsl_ma87d_ciface.o) \
                   $(HCLGD)(hsl_ma97d_ciface.o) \
                   $(HCLGD)(hsl_mi20d_ciface.o) \
                   $(HCLGD)(hsl_mi28d_ciface.o)
hsl_ciface_single_64: $(HCLGS64)(hsl_ma48s_ciface.o) \
                      $(HCLGS64)(hsl_ma57s_ciface.o) \
                      $(HCLGS64)(hsl_ma77s_ciface.o) \
                      $(HCLGS64)(hsl_ma86s_ciface.o) \
                      $(HCLGS64)(hsl_ma87s_ciface.o) \
                      $(HCLGS64)(hsl_ma97s_ciface.o) \
